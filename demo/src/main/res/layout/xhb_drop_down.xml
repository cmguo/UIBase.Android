<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <import type="com.xhb.uibase.demo.components.simple.XHBDropDownFragment.Styles" />

        <variable
            name="styles"
            type="Styles" />

        <import type="com.xhb.uibase.demo.components.simple.XHBDropDownFragment.Model" />

        <variable
            name="model"
            type="Model" />

    </data>

    <com.xhb.uibase.widget.XHBDropDown
        android:id="@+id/dropDown"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:titles="@{model.titles}"
        app:icons="@{model.icons}"/>

</layout>